{
  "name": "NASM Assembly",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "doinkythederp.nasm-language-support",
        "ms-vscode.cpptools"
